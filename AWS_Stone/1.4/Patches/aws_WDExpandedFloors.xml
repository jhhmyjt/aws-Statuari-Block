            <value>
                <TerrainDef ParentName="WDRoughStoneTileBase">
                    <defName>awsRoughPracticalTile</defName>
                    <label>rough statuari tile</label>
                    <color>(255,254,249)</color>
                    <costList>
                        <awsPracticalStoneBlock>4</awsPracticalStoneBlock>
                    </costList>
                </TerrainDef>
                <TerrainDef ParentName="WDStoneTileBase">
